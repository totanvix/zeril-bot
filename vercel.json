{
  "routes": [
    {
      "src": "/url/(.*)",
      "dest": "/api/url/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "55 13 * * *"
    }
  ],
  "github": {
    "enabled": false,
    "silent": true
  }
}
